<% provide :title, t(".home") %>
  <body>
    <div class="center jumbotron">
	    <h1><%= t(".welcome") %></h1>
	  </div>
    <div class="col-md-12">
    <% if @tour_items.any? %>
      <h3>Tours (<%= @tour_items.count %>)</h3>
      <ol class="tours">
        <%= render @tour_items %>
      </ol>
      <%= will_paginate @tour_items %>
    <% end %>
  </div>
  </body>
