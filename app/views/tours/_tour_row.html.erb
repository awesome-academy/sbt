<% if tour_row.start_day.to_i > Time.now.to_i %>
<tr>
<% else %>
<tr class = "danger">
<% end %>
  <td class = "text-center"><%= tour_row_counter +=1 %></td>
  <td class = "text-center"><%= link_to tour_row.title, tour_row %></td>
  <td class = "text-center"><%= tour_row.account.email %></td>
  <td class = "text-center"><%= tour_row.start_day.to_date %></td>
  <td class = "text-center"><%= tour_row.end_day.to_date %></td>

  <th class = "text-center"><%= tour_row.status %></th>
  <td class = "text-center"><%= link_to t("delete"), tour_row, method: :delete, data: { confirm: "Sure?"} %></td>
</tr>
